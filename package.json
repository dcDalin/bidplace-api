{
  "name": "bidplace-api",
  "version": "1.0.0",
  "description": "bidplace api",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "npm run clean && babel src --out-dir build",
    "clean": "rm -rf build",
    "start": "npm run build && node ./build/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "prettier:base": "prettier --parser --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\""
  },
  "lint-staged": {
    "src/**/*.{js}": [
      "eslint . --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcDalin/bidplace-api.git"
  },
  "author": "mcdalinoluoch@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dcDalin/bidplace-api/issues"
  },
  "homepage": "https://github.com/dcDalin/bidplace-api#readme",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  }
}
